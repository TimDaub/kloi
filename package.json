  "author": "Tim Daubensch√ºtz <tim@daubenschuetz.de> (https://timdaub.github.io)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/TimDaub/kloi/issues"
  },
  "homepage": "https://github.com/TimDaub/kloi#readme",
  "dependencies": {
    "directory-tree": "2.2.5",
    "yup": "0.32.8"
